{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8 // or 2017
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-console": "off"
    }
}